  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsc -w",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.8"
  }
}

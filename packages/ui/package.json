{
  "name": "@figkit/ui",
  "version": "0.1.0",
  "description": "React component library with design tokens and multi-brand/theme support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsc -w",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hazenbox/FigKit.git",
    "directory": "packages/ui"
  }
}

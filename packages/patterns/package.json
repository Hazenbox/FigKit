{
  "name": "@figkit/patterns",
  "version": "0.1.0",
  "description": "Composed UI patterns built on top of @figkit/ui components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts && node -e \"const fs = require('fs'); if (!fs.existsSync('dist/index.css')) { fs.writeFileSync('dist/index.css', '/* Patterns package styles - currently empty */', 'utf8'); }\"",
    "dev": "tsc -w",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@figkit/ui": "^0.1.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "react",
    "patterns",
    "ui",
    "design-system",
    "components"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hazenbox/FigKit.git",
    "directory": "packages/patterns"
  }
}
